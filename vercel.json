{
  "version": 2,
  "functions": {
    "api/bot.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/bot.js"
    }
  ],
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    }
  ]
}
